{
    "name": "react-boilerplate",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:5000",
    "dependencies": {
        "axios": "^0.18.0",
        "dotenv": "4.0.0",
        "dotenv-expand": "4.2.0",
        "koa": "^2.5.0",
        "koa-body": "^2.5.0",
        "koa-bodyparser": "^4.2.1",
        "koa-compress": "^3.0.0",
        "koa-respond": "^2.1.0",
        "koa-router": "^7.4.0",
        "koa2-cors": "^2.0.5",
        "promise": "8.0.1",
        "react": "^16.5.2",
        "react-dev-utils": "^5.0.2",
        "react-dom": "^16.5.2",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "styled-components": "^4.0.2"
    },
    "scripts": {
        "dev": "concurrently \"npm run start:app\" \"npm run start:server\" ",
        "build:prod": "npm run build:app && npm run build:server",
        "build:app": "node config/scripts/build.js",
        "build:server": "webpack --config config/webpack.config.server.js",
        "start:app": "node config/scripts/start.js",
        "start:server": "webpack --config config/webpack.config.server.js --watch",
        "test:unit": "CI=true node config/scripts/test.js --env=jsdom",
        "test:int": "jest __tests__"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.14",
        "@types/jest": "^23.3.7",
        "@types/koa": "^2.0.44",
        "@types/koa-bodyparser": "^5.0.1",
        "@types/koa-compress": "^2.0.8",
        "@types/koa-router": "^7.0.27",
        "@types/koa2-cors": "^2.0.1",
        "@types/node": "^10.12.0",
        "@types/react": "^16.4.18",
        "@types/react-dom": "^16.0.9",
        "@types/react-redux": "^6.0.9",
        "@types/react-router-dom": "^4.3.1",
        "@types/styled-components": "^4.0.1",
        "@types/supertest": "^2.0.2",
        "autoprefixer": "7.1.6",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-preset-react-app": "^3.1.2",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "concurrently": "^4.0.1",
        "css-loader": "0.28.7",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.6.0",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.5",
        "fork-ts-checker-webpack-plugin": "^0.2.8",
        "fs-extra": "3.0.1",
        "html-webpack-plugin": "2.29.0",
        "jest": "20.0.4",
        "nodemon-webpack-plugin": "^3.0.1",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "raf": "3.4.0",
        "resolve": "1.6.0",
        "source-map-loader": "^0.2.1",
        "style-loader": "0.19.0",
        "supertest": "^3.0.0",
        "sw-precache-webpack-plugin": "0.11.4",
        "ts-jest": "22.0.1",
        "ts-loader": "^2.3.7",
        "ts-node": "^3.2.0",
        "tsconfig-paths-webpack-plugin": "^2.0.0",
        "tslint": "^5.7.0",
        "tslint-config-prettier": "^1.10.0",
        "tslint-react": "^3.2.0",
        "typescript": "^3.1.3",
        "uglifyjs-webpack-plugin": "1.2.5",
        "url-loader": "0.6.2",
        "webpack": "3.8.1",
        "webpack-dev-server": "2.11.3",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!**/*.d.ts"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "<rootDir>/config/setupTests.js"
        ],
        "testMatch": [
            "<rootDir>/__tests__/*.(j|t)s?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "web.js",
            "js",
            "web.jsx",
            "jsx",
            "json",
            "node",
            "mjs"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "tsconfig.test.json"
            }
        }
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    }
}
